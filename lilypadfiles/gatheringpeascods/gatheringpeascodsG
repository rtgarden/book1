\header { title = "Gathering peascods" composer = "traditional" }
\relative c' {
\key g\major
\time 4/4
\repeat volta 2 {
d'2 d4 d4
b4. c8 d4 d4
e4 d4 c4 b4
a2. b4
a4 g4 g4 fis4 }
\alternative { {g1} {g2. g4} }
\repeat volta 2 {
fis4 d4 fis4 g4
a2 b4 a4
g8 a8 b4 a4 g4 
fis2. fis4
e4 d4 e4. d8 }
\alternative {{d2. g4}{d2. d'4} }
\repeat volta 2 {
b4 g4 g4 a8 b8
c2. c4
b4 g4 g4 a8 b8
c2. d4 
b4 g4 g4 a8 b8
c4. d8 e4 d8 c8
b4 c8 b8 a4. g8 }
\alternative { {g2. d'4}{g,2. r4} } 
}
