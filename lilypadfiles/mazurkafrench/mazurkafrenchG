\version "2.12.3"
\header { title = "Mazurka" composer = "traditional Central France" }
\relative c' {
\key g \major
\time 3/4
\tempo 4 = 120
\repeat volta 2 {
d4 g8 fis8 g4
d4 b'8 a8 b4 }
\alternative { { c8 b8 a8 g8 fis8 g8 a8 g8 fis8 e8 d4 }{ c'8 b8 a8 g8 fis8 a8 g2 b8 c8 }} \break

\repeat volta 2 {
d4 d8 e8 d8 c8 
b4 g4 a8 b8 }
\alternative { { c4 c8 d8 c8 b8 a2 b8 c8 }{ c8 b8 a8 g8 fis8 a8 g2. } }
}
