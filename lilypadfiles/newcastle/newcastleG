\version "2.12.3"
\header { title = "Newcastle" composer = "traditional" }
\relative c' {
\key g \major
\time 4/4 
\repeat volta 2 {
b'4 d4 g,4 a4
g4. a8 g4 d4
b'4 d4 g,4 d'4
e4 g2 fis8 e8
d4 b4 a4. g8 }

\alternative { {g1} {g2.}}

\repeat volta 2 {
\partial 4
e'8 fis8

g8 fis8 e8 d8 g4. b,8
a4 g'2 a,4
g4. a8 b4 fis4 \break
e4 e'2 fis4 \grace { e16[ fis16] }

g8 fis8 e8 d8 g4. b,8
a4 a4 c4. d8
e4 b4 a4. g8 }

\alternative { {g2.} {g2 ~ g2} } \bar "|."
    
  
}


