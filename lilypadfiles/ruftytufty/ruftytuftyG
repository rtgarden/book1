\version "2.12.3"
\header { title = "Rufty Tufty"
  composer= "traditional" }
\relative c' {
\key g \major
\time 4/4
\repeat volta 2 { 
d'2 d4 e4
fis2 e4 fis4
g4 g4 fis4. e8 }
\alternative { {d1} {d2.} }
\repeat volta 2 {
\partial 4
e8 fis8 
g4 fis4 e4 d4 \break
d4 c4 b4. c8 
d4 d4 c4 b4 }
\alternative { {a2 g4} {a2 g2} } 
\time 4/4
\partial 1
\repeat volta 2 {
b4. c8 d4 d4 |
e4 c4 d2 | \break
b4. c8 d4 d4 |
e4 c4 d4 b8 c8 |
d4 d4 c4 b4 |
a2 g2 }
}
