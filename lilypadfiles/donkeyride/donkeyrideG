\version "2.12.3"
\header { title = "Donkey Ride" composer = "traditional hornpipe" }
\relative c' {
\key g \major
\time 4/4
\repeat volta 2 {
e4 g4 b4 b8 b8
c4 a4 b4 b8 b8 }
\alternative { { b4 a4 a4 g8 a8 b4 a4 a4 fis4 }{ b4 g4 a4 fis4 e4 d4  e2} }
\repeat volta 2 {
e'4 e4 d4 b4
c4 a4 b2
b4 a4 a4 g8 a8 
b4 a4 a2
c4 c4 d4 b4 
c4 a4 b2
b4 g4 a4 fis4
e4 d4 e2 }
}
